// .env.example - Updated with all necessary environment variables
APP_NAME="PAMDes Management"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_TIMEZONE=Asia/Jakarta
APP_URL=https://pamdes.kecamatanbayan.id
APP_DOMAIN=kecamatanbayan.id

LOG_CHANNEL=stack
LOG_STACK=single
LOG_LEVEL=info

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=pamdes_db
DB_USERNAME=pamdes_user
DB_PASSWORD=secure_password

# Village System Integration
VILLAGE_SYSTEM_URL=https://kecamatanbayan.id
VILLAGE_SYSTEM_API_KEY=your_village_api_key_here
VILLAGE_SYSTEM_TIMEOUT=10

# PAMDes API Configuration
PAMDES_API_KEY=your_pamdes_api_key_here
PAMDES_URL=https://pamdes.kecamatanbayan.id

# Feature Flags
VILLAGE_AUTO_SYNC=true
VILLAGE_NOTIFICATIONS=true
VILLAGE_SSO_ENABLED=true

# Cache Configuration
CACHE_STORE=database
SESSION_DRIVER=database
QUEUE_CONNECTION=database

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=smtp.example.com
MAIL_PORT=587
MAIL_USERNAME=noreply@pamdes.kecamatanbayan.id
MAIL_PASSWORD=mail_password
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@pamdes.kecamatanbayan.id
MAIL_FROM_NAME="PAMDes Management"

# Filament Configuration
FILAMENT_DOMAIN=pamdes.kecamatanbayan.id

# File Storage
FILESYSTEM_DISK=local

# Broadcasting
BROADCAST_DRIVER=log
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

# Redis (optional)
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Sentry (optional for error tracking)
SENTRY_LARAVEL_DSN=
SENTRY_TRACES_SAMPLE_RATE=1.0